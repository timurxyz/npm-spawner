{
  "name": "npm-spawner",
  "version": "1.0.5-d",
  "description": "Little sugar around node.proc.spawn to load npm/yarn/pnpm with commands/scripts programmatically [ts/js]",
  "license": "MIT",
  "type": "module",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": "./lib/index.js",
  "scripts": {
    "build": "tsc --build",
    "publish:npm": "npm publish --access public",
    "publish:local": "yalc push"
  },
  "devDependencies": {
    "@types/node": "latest",
    "typescript": "latest"
  },
  "files": [
    "lib/**"
  ],
  "keywords": ["npm","api","run","yarn"],
  "author": {
    "name": "timur x",
    "url": "https://github.com/timurxyz"
  },
  "bugs": {
    "url": "https://github.com/timurxyz/npm-spawner/issues"
  }
}
